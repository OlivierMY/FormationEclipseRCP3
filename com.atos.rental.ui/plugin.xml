<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="ColorPalette" name="ColorPalette" schema="schema/ColorPalette.exsd"/>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="com.atos.rental.ui.view.RentalPropertiesView"
            icon="icons/Rentals.png"
            id="com.atos.rental.ui.rentalProperties"
            name="%view.name"
            restorable="true">
      </view>
      <view
            class="com.atos.rental.ui.view.AgencyView"
            icon="icons/Agency.png"
            id="com.atos.rental.ui.Agency"
            name="%view.name.0"
            restorable="true">
      </view>
      <view
            class="com.atos.rental.ui.view.CustomerView"
            icon="icons/Customers.png"
            id="com.atos.rental.ui.CustomerView"
            name="CustomerView"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="com.atos.rental.ui.perspective.RentalPerspective"
            icon="icons/Rentals.png"
            id="com.atos.rental.ui.perspective.RentalPerspective"
            name="%perspective.name">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="com.atos.rental.ui.pref.palette"
            class="com.atos.rental.ui.pref.DefaultColor"
            id="com.atos.rental.ui.pref.colors"
            name="%page.name">
      </page>
      <page
            class="com.atos.rental.ui.pref.PalettePref"
            id="com.atos.rental.ui.pref.palette"
            name="Palette">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="com.atos.rental.ui.perf.PreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="com.atos.rental.ui.commandhelloworld"
            name="%command.name">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="com.atos.rental.ui.handler.HelloWorldHandler"
            commandId="com.atos.rental.ui.commandhelloworld">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <command
               commandId="com.atos.rental.ui.commandhelloworld"
               icon="icons/sample.gif"
               label="%command.label"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="menu:org.eclipse.ui.views.ResourceNavigator?after=additions">
         <command
               commandId="com.atos.rental.ui.commandhelloworld"
               icon="icons/sample.gif"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:com.atos.rental.ui.Agency?after=additions">
         <menu
               commandId="com.atos.rental.ui.commandhelloworld"
               icon="icons/Agency.png"
               label="%menu.label">
            <command
                  commandId="com.atos.rental.ui.commandhelloworld"
                  icon="icons/sample.gif"
                  label="%command.label.0"
                  style="push">
            </command>
         </menu>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="com.atos.rental.ui.commandhelloworld"
            contextId="org.eclipse.ui.contexts.dialogAndWindow"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="F6">
      </key>
   </extension>
   <extension
         point="com.atos.rental.ui.ColorPalette">
      <palette
            colorPaletteClass="com.atos.rental.ui.colorPalette.FixeNoir"
            id="com.atos.rental.ui.colorPalette.FixeNoir"
            name="FixeNoir">
      </palette>
      <palette
            colorPaletteClass="com.atos.rental.ui.colorPalette.Default"
            id="com.atos.rental.ui.colorPalette.Default"
            name="Default">
      </palette>
   </extension>

</plugin>
