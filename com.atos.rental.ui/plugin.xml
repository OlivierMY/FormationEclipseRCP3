<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="com.atos.rental.ui.view.RentalPropertiesView"
            icon="icons/Rentals.png"
            id="com.atos.rental.ui.rentalProperties"
            name="RentalProperties"
            restorable="true">
      </view>
      <view
            class="com.atos.rental.ui.view.AgencyView"
            icon="icons/Agency.png"
            id="com.atos.rental.ui.Agency"
            name="AgencyView"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="com.atos.rental.ui.perspective.RentalPerspective"
            icon="icons/Rentals.png"
            id="com.atos.rental.ui.perspective.RentalPerspective"
            name="RentalPerspective">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="com.atos.rental.ui.pref.Colors"
            id="com.atos.rental.ui.pref.colors"
            name="Colors">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="com.atos.rental.ui.perf.PreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="com.atos.rental.ui.commandhelloworld"
            name="HelloWorld">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="com.atos.rental.ui.handler.HelloWorldHandler"
            commandId="com.atos.rental.ui.commandhelloworld">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <command
               commandId="com.atos.rental.ui.commandhelloworld"
               icon="icons/sample.gif"
               label="HelloWorld"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="menu:org.eclipse.ui.views.ResourceNavigator?after=additions">
         <command
               commandId="com.atos.rental.ui.commandhelloworld"
               icon="icons/sample.gif"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:com.atos.rental.ui.Agency?after=additions">
         <menu
               commandId="com.atos.rental.ui.commandhelloworld"
               icon="icons/Agency.png"
               label="HelloSousMenu">
            <command
                  commandId="com.atos.rental.ui.commandhelloworld"
                  icon="icons/sample.gif"
                  label="HelloWorld"
                  style="push">
            </command>
         </menu>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="com.atos.rental.ui.commandhelloworld"
            contextId="org.eclipse.ui.contexts.dialogAndWindow"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="F6">
      </key>
   </extension>

</plugin>
